{
    "name": "dankempster/template-php-component",
    "description": "A template workspace for developing a new component/package. Includes configuration for travis-ci.org, Scrutinizer.com and Coveralls.io.",
    "type": "project",
    "keywords": ["workspace template", "travis-ci", "scrutinizer", "coveralls"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Kempster",
            "email": "dev@dankempster.co.uk",
            "homepage": "https://github.com/dankempster"
        }
    ],

    "require": {
        "php": ">=5.4"
    },

    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "satooshi/php-coveralls": "dev-master"
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },

    "minimum-stability": "dev"
}
